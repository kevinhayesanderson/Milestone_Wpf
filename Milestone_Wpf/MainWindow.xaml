<Window x:Class="Milestone_Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Milestone_Wpf"
        mc:Ignorable="d"
        Title="Milestone" Height="300" Width="500">
    <DockPanel Margin="10">
        <WrapPanel HorizontalAlignment="Center" DockPanel.Dock="Top" Margin="0,0,0,10">
            <Button Name="btnOpenFile" Click="BtnOpenFiles_Click">Open files</Button>
        </WrapPanel>
        <ListBox Name="logList" />
    </DockPanel>
</Window>
